{
    "version": 2,
    "builds": [
      {
        "src": "backend/src/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/react"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/src/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "backend/src/public/$1"
      }
    ]
  }
  